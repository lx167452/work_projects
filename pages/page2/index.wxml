                    </picker>
                    <view class="triangle_icons"></view>
                </view>
            </view>
        </view>
    </view>

    <!-- 滞留扣分 -->
    <view class="item_cell_box">
        <view class="menu_cell_title">滞留扣分</view>
        <view class="menu_cell_list">
            <view class="cell_item_bar">
                <view class="cell_item_title">个人原因不报到</view>
                <view class="cell_item">
                    <picker class="item_select" bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
                        无
                    </picker>
                    <view class="triangle_icons"></view>
                </view>
            </view>
        </view>
    </view>
    <view class="sub_btn">提交</view>
</view>