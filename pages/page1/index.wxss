page {
    background: #F4F4F4;
}

.container {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.head_nav {
    width: 100%;
    height: 360rpx;
}

.main_container {
    position: absolute;
    top: 295rpx;
    margin: 0 auto;
    padding: 0 30rpx;
}

.main_container .item_cell {
    background: #FFFFFF;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 32rpx 36rpx 52rpx;
    margin-bottom: 24rpx;
}

.main_container .item_block {
    display: flex;
    align-items: center;
}

.main_container .item_cell .item_cell_icon {
    width: 82rpx;
    height: 95rpx;
}

.main_container .item_cell .item_cell_text {
    font-size: 32rpx;
    color: #333333;
    margin-left: 30rpx;
}

.main_container .item_cell .right_arrow {
    width: 19rpx;
    height: 34rpx;
}

.main_container .tooltip {
    line-height: 36rpx;
    margin: 184rpx auto 0;
    text-align: center;
    font-size: 26rpx;
    color: #999999;
    padding: 0 86rpx;
}

.main_container .copyright {
    font-size: 24rpx;
    color: #E52229;
    margin: 12rpx auto 0;
    text-align: center;
}